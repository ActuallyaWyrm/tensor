{
  "name": "tfjs-models",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "license": "Apache-2.0",
  "private": true,
  "engines": {
    "node": ">=8.9.0"
  },
  "dependencies": {
    "@tensorflow-models/posenet": "file:../dist",
    "@tensorflow/tfjs-backend-webgl": "^3.3.0",
    "@tensorflow/tfjs-converter": "^3.3.0",
    "@tensorflow/tfjs-core": "^3.3.0",
    "stats.js": "^0.17.0"
  },
  "scripts": {
    "watch": "esbuild camera.js coco.js --sourcemap --target=es6 --sources-content=true --external:os --bundle --resolve-extensions=.mjs,.js --servedir=.",
    "build": "./scripts/build-demo.sh",
    "lint": "eslint .",
    "link-local": "yalc link",
    "build-dep": "cd ../ && yarn && yarn build"
  },
  "browser": {
    "crypto": false
  },
  "devDependencies": {
    "clang-format": "~1.2.2",
    "dat.gui": "^0.7.2",
    "esbuild": "^0.12.11",
    "eslint": "^4.19.1",
    "eslint-config-google": "^0.9.1",
    "rimraf": "^3.0.2",
    "yalc": "~1.0.0-pre.50"
  },
  "resolutions": {
    "is-svg": "4.3.1"
  },
  "eslintConfig": {
    "extends": "google",
    "rules": {
      "require-jsdoc": 0,
      "valid-jsdoc": 0
    },
    "env": {
      "es6": true
    },
    "parserOptions": {
      "ecmaVersion": 8,
      "sourceType": "module"
    }
  },
  "eslintIgnore": [
    "dist/"
  ]
}
